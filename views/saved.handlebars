       <html>
<head>

	<title>Neutral Noggin - Saved</title>

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="css/neutral-noggin.css" rel="stylesheet">

	<!-- Link css files -->
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap-theme.css"rel="stylesheet">
	<link href="css/neutral-noggin.css" rel="stylesheet">

	<!-- Link google fonts -->
	<link href="https://fonts.googleapis.com/css?family=Cabin|Montserrat|Roboto+Slab&display=swap" rel="stylesheet">

	<!-- Script for linking tooltaskbars html-->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

	<!-- Link scripts -->
	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/neutral-noggin.js"></script>


	<script>
		$(document).ready(function(){
			$("#topbanner").load("topbanner.html");
		});
	</script>


	<!-- Google Analytics -->
	<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

	ga('create', 'UA-159295350-1', 'auto');
	ga('send', 'pageview');
	</script>
	<!-- End Google Analytics -->
</head>

<body>
	<div id="topbanner"></div>

	<div class="tilt" id="{{userTilt}}" name="tilt"></div>
	<div class="userlogin" id="{{loggedInProfile}}" name="userlogin"></div>
	{{#if altView}}
		<div class="version" id="B" name="ver"></div>
	{{else}}
		<div class="version" id="A" name="ver"></div>
	{{/if}}

	<div class="myTaskbar">
		<div class="taskbarTable">
			<table>
				<tr>
		        	<td>
		        		<a id="taskButtonH" href="home?ut=0">Daily News</a>
		        	</td>
		        	<td>
		        		<a id="taskButtonS" href="saved?ut=0" class="selected">Saved</a>
		        	</td>
		        	<td>
		        		<a id="taskButtonM" href="more?ut=0">More</a>
		        	</td>
		      	</tr>
		    </table>
		</div>
	</div>

	<div class="container" id="article-list">
		{{#each userSaved}}
		<a class="container link index" id="{{tilt}}" href={{URL}} target="_blank">

			<div class="container article-container">

				<div class="container headline-container">
					<h3 class="headline">{{headline}}</h3>
				</div>

				<div class="container image-container">
					<img src={{imageURL}} class="img-responsive">
				</div>

			</div>

		</a>

		<div class="deleteArticleButton" id="{{URL}}">
			<button type="button">Delete</button>
		</div>

		<div class="container article-break"></div>

		{{/each}}
	</div>
	<hr>
	<p id="saveMsg" style="font-size: 14px; color: #787878; margin: 10px">Any saved articles will show up here!</p>
	<hr>

</body>
</html>